##警情信息 input  type 管理


##警情查询参数
input IncidentQueryInputInfo{
   scopeSquadronId : String    ##查询范围辖区Id 必填
   scopeType : Int     ##查询范围辖区类型 0根机构 1非根机构
   squadronIds : [String]    ##辖区id集合
   keyword  : String    ##模糊匹配关键字（警情地址模糊匹配）
   receiverObjectId  : String    ##接收对象id
   incidentTypeCodes : [String]    ##警情类型编码集合
   incidentLevelCodes : [String]    ##警情等级编码集合
   incidentStateCodes : [String]    ##警情状态编码集合
   filterIncidentIds : [String]    ##过滤警情id
   startTime : Long      ##查询开始时间
   endTime : Long      ##查询结束时间
   whetherKeyUnit : Boolean     ##是否查询重点单位
   whetherImport : Boolean     ##是否查询重大案件
   whetherPage : Boolean     ##是否分页查询 默认分页
   pagination : PaginationInputInfo     ##分页条件
}

##警情列表分页
type PaginationIncidentBean{
    pagination : Pagination    ##分页
    list : [IncidentBrieflyBean]    ##数据集合
}


##警情列表简要Bean
type IncidentBrieflyBean {
	id : String   ## 主键
	incidentNumber : String   ## 案件编号（日期坐席工号）
      squadronOrganizationId  : String   ## 主管中队
   squadronOrganizationName     : String   ## 主管中队名称
    brigadeOrganizationId    : String   ## 所属大队
    brigadeOrganizationName    : String   ## 所属大队名称
     districtCode   : String   ##  行政区编码
     districtName   : String   ##  行政区名称
     registerModeCode   : String   ## 立案方式代码
     registerModeName   : String   ## 立案方式代码
     registerIncidentTime   : Long   ## 立案时间
     crimeAddress   : String   ## 案发地址
      longitude  : String   ## 经度
      latitude  : String   ## 纬度
     height   : String   ## 高度
     incidentTypeCode   : String   ## 案件类型代码
    incidentTypeName    : String   ## 案件类型名称
     incidentTypeSubitemCode   : String   ## 案件类型子项代码（细类）（案件类型备用）
      incidentTypeSubitemName  : String   ## 案件类型子项名称（细类）（案件类型备用）
      incidentLevelCode  : String   ## 案件等级代码
      incidentLevelName  : String   ## 案件等级名称
      disposalObjectCode  : String   ## 处置对象代码
     disposalObjectName   : String   ## 处置对象名称
      incidentStatusCode  : String   ## 案件状态代码
      incidentStatusName  : String   ## 案件状态名称
      incidentNatureCode  : String   ## 案件性质代码 1 实警；0 虚警
      incidentNatureName  : String   ## 案件性质名称
       whetherImportSign : Int   ## 是否重大案件标识
       mergeStatus : Int   ## 合并状态
      whetherMainMerge  : Int    ## 是否为合并主案件
      registerSeatNumber  : String   ## 立案接警坐席号
       acceptancePersonNumber : String   ## 接警员工号
       acceptancePersonName : String   ## 接警员姓名
      dispatchOrganization  : String   ##出动机构
     dispatchVehicle   : String   ## 出动车辆
      incidentGroupId  : String   ##案件群组id 如果有多个群组 多个群组逗号分隔
      whetherFocusOn  : Int    ##是否关注 0不关注 1 关注 默认 0 不关注
      whetherImportant  : Int      ##重要警情标识 0非重要 1重要 默认0非重要
      attachmentNum  : Int      ##附件数量
      alarmPhone  : String   ##报警电话
     relayRecordNumber   : String   ##录音号 （案件主案件录音号）
	alarmModeCode : String   ## 报警方式代码
	alarmModeName : String   ## 报警方式名称

	alarmPersonPhone : String ## 报警人联系电话
    alarmPersonName : String   ##报警人姓名

      soundRecordList  : [SoundRecordBean]  ##录音文件信息
}


